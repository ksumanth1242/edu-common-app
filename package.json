{
  "name": "edu-common-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.18",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--max-old-space-size=4096 && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:coverage": "react-scripts test --coverage --silent --watchAll=false",
    "test:ci": "react-scripts test --coverage --silent --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "audit:security": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "commit": "git add . && git commit",
    "prepare": "husky install",
    "checks:status": "node scripts/check-config.js",
    "checks:demo": "node scripts/demo-checks.js",
    "checks:enable-all": "node scripts/manage-checks.js enable-all",
    "checks:disable-all": "node scripts/manage-checks.js disable-all",
    "checks:enable-eslint": "node -e \"require('fs').writeFileSync('.env.local', 'ENABLE_ESLINT=true\\n', {flag: 'a'})\"",
    "checks:disable-eslint": "node -e \"require('fs').writeFileSync('.env.local', 'ENABLE_ESLINT=false\\n', {flag: 'a'})\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts",
      "!src/config/**",
      "!src/constants/**",
      "!src/types/**",
      "!src/utils/**",
      "!src/services/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "lint-staged": "./lint-staged.config.js",
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "customize-cra": "^1.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.0.0",
    "npm-audit-reporter": "^1.0.3",
    "prettier": "^2.8.0",
    "react-app-rewired": "^2.2.1",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
